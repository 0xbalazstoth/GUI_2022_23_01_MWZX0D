<Window x:Class="Gunner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gunner"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="1024" MinWidth="1280" KeyDown="Window_KeyDown" WindowStyle="ThreeDBorderWindow" WindowStartupLocation="CenterScreen">
    <DockPanel LastChildFill="True">
        <TextBlock Text="GAME TITLE" DockPanel.Dock="Top"/>
        <Button Content="BUTTON" DockPanel.Dock="Top"></Button>
        <WindowsFormsHost PreviewMouseWheel="SfmlSurfaceHost_PreviewMouseWheel" x:Name="SfmlSurfaceHost" />
    </DockPanel>
</Window>
